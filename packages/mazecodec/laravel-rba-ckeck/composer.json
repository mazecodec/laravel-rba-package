{
    "name": "mazecodec/laravel-rba-check",
    "description": "Laravel RBA middleware",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mazecodec\\LaravelRbaCheck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mazecodec\\LaravelAzureBlobStorage\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Mazecodec",
            "email": "mazecode.tec@gmail.com"
        }
    ],
    "version": "1.0.0",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "10.4.x-dev"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mazecodec\\LaravelRbaCheck\\RBACheckServiceProvider"
            ]
        }
    }
}
